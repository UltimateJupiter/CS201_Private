
public class CirclesCountry {
	
	public static boolean border_detect(double cx, double cy, double cr, double px, double py ) {
	    double dist = Math.sqrt(Math.pow(cx - px, 2) + Math.pow(cy - py, 2));
	    boolean result = dist < cr;
		return result;
	}
	
