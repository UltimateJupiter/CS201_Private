		return sum;
    }
    
    public static void main(String[] args){
    	int x1 = 1, y1 = 2, x2 = 3, y2 = 4, r = 8;
    	boolean res = border_detect(x1, y1, r, x2, y2);
    	
        System.out.println(res);
    }
 }